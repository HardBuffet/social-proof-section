include ../../pug/abstracts/variables/bg-pattern.js.pug


div(class=bgPattern.getComponent().name)
  picture(class=bgPattern.getComponent().el.top)
    source(srcset="images/bg-pattern-top-desktop.svg" media="(min-width: 1440px)")
    img(src="images/bg-pattern-top-mobile.svg" alt="")
  picture(class=bgPattern.getComponent().el.bottom)
    source(srcset="images/bg-pattern-bottom-desktop.svg" media="(min-width: 1440px)")
    img(src="images/bg-pattern-bottom-mobile.svg" alt="")
